{
    "name": "react-material-ui-carousel-mui-v7-react-19",
    "version": "4.0.0",
    "license": "MIT",
    "description": "React Material Ul Carousel from Learus with updated dependencies to support @mui v7 and React 19.",
    "homepage": "https://github.com/jbyerline/react-material-ui-carousel",
    "repository": {
        "type": "git",
        "url": "https://github.com/jbyerline/react-material-ui-carousel"
    },
    "keywords": [
        "react",
        "material",
        "material ui",
        "carousel",
        "gallery"
    ],
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "/dist"
    ],
    "babel": {
        "presets": [
            "react",
            "env",
            "stage-2"
        ]
    },
    "scripts": {
        "build": "npx rimraf dist && npm run build:cjs",
        "build:cjs": "tsc --module commonjs --outDir dist",
        "build_old": "NODE_ENV=production && npx rimraf dist && npx mkdirp dist && npx babel ./src --out-dir dist --copy-files"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@emotion/react": "^11.11.0",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^7.1.0",
        "@mui/material": "^7.1.0",
        "@mui/system": "^7.1.0",
        "framer-motion": "^11.0.0"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.24.0",
        "@babel/preset-react": "^7.24.0",
        "@babel/preset-typescript": "^7.24.0",
        "@types/react": "^18.2.42",
        "@types/react-dom": "^18.2.15",
        "babel-loader": "^9.1.3",
        "react-scripts": "^5.0.1",
        "rimraf": "^5.0.5",
        "typescript": "^5.4.5"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0",
        "@mui/material": "^7.0.0",
        "@mui/icons-material": "^7.0.0",
        "@mui/system": "^7.0.0",
        "@emotion/react": "^11.10.0",
        "@emotion/styled": "^11.10.0"
    }
}
